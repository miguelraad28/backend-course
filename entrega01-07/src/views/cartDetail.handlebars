<div class="cartContainer">
    <div class="cartProductsContainer">
        <h1>Detalle de mi carrito de compras</h1>
        {{#if hayProductos}}
        {{#each p}}
        <div class="productCard">
            <h3>{{this.product.title}}</h3>
            <p>
                $ {{this.product.price}}
            </p>
            <p>Cantidad: {{this.quantity}}</p>
        </div>
        {{/each}}
        {{else}}
        <h2>El carrito está vacío</h2>
        {{/if}}
    </div>
</div>